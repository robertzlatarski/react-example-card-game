{
  "name": "ts-iso-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts-ts": "2.16.0",
    "styled-components": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "compile-server":
      "rm -rf build/dist && tsc --project src/server/tsconfig.json",
    "watch-server": "npm run compile-server  -- --watch",
    "start-server":
      "npm run compile-server && nodemon build/dist/server/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.5",
    "ts-node": "^6.0.5",
    "typescript": "^2.8.3"
  }
}
